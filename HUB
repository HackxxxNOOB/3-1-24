                                            if
                                                (v.Name == "Reborn Skeleton" or v.Name == "Living Zombie" or
                                                    v.Name == "Demonic Soul" or
                                                    v.Name == "Posessed Mummy") and
                                                    (v.HumanoidRootPart.Position - PosMonBone.Position).Magnitude <=
                                                        _G.BringMode and
